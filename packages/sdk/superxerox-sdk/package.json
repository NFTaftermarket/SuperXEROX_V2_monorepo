{
  "name": "superxerox-sdk",
  "version": "0.0.2",
  "description": "Superxerox v2 (NFT copy machine) js-sdk library",
  "keywords": [
    "nft",
    "ethereum",
    "blockchain",
    "superfluid",
    "ipfs",
    "solidity",
    "dapp"
  ],
  "main": "dist/superxerox.js",
  "module": "dist/index.modern.js",
  "source": "src/superxerox.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "types": "dist/superxerox.d.ts",
  "files": [
    "dist"
  ],
  "author": "Ming-der Wang; <mingderwang@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/mingderwang/superxerox-sdk"
  },
  "license": "MIT",
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "lint": "eslint --fix 'src/**/*.ts'",
    "prebuild": "rimraf dist",
    "build:docs": "typedoc --out docs --theme minimal src",
    "test": "jest --config jest.config.ts",
    "test:coverage": "jest --config jest.config.ts --coverage",
    "test:watch": "jest --config jest.config.ts --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy": "npm run test:prod && np",
    "show:docs": "npm run build:docs && http-server docs"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@alch/alchemy-web3": "^1.1.2",
    "@babel/parser": "^7.15.2",
    "@babel/types": "^7.15.0",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@jest/types": "^27.0.6",
    "@superfluid-finance/js-sdk": "^0.4.3",
    "@types/jest": "26.0.19",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "create-web3-provider": "^1.6.2",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "http-server": "^13.0.0",
    "husky": "^5.1.3",
    "jest": "26.6.3",
    "jest-config": "26.6.3",
    "jest-diff": "^27.0.6",
    "lint-staged": "^10.5.3",
    "microbundle-crl": "^0.13.11",
    "prettier": "^2.2.1",
    "pretty-format": "^27.0.6",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.0",
    "swarm-js": "^0.1.40",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.28",
    "typescript": "^4.3.0-beta"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.4.3",
    "ethers": "^5.4.4",
    "request-promise": "^4.2.6"
  }
}
